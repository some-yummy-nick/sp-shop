{% macro menu(active=false) %}
	<nav class="menu">
        <ul class="menu__list">
            {% for item in menuItems %}
            <li class="menu__item">
                <a href="{{ item.path }}" class="menu__link">{{ item.name }}</a>
            </li>
            {% endfor %}
        </ul>
	</nav>
{% endmacro %}
